{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "sourceMap": true,
    "outDir": "./dist/",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "vuex": ["node_modules/vuex/types/index.d.ts"],
      "vue": ["node_modules/vue/types/index.d.ts"]
    },
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es2017",
      "dom"
    ]
  }
}